source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

target 'Tumblr' do
# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
use_frameworks!

# Pods for Tumblr
pod 'AlamofireImage'

target 'TumblrTests' do
inherit! :search_paths
# Pods for testing
end

target 'TumblrUITests' do
inherit! :search_paths
# Pods for testing
end

end



